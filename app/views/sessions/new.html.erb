<h1> Login to DagolfaApp</h1> 

<% if flash["error"] %>
<div class="error"><%= flash["error"] %></div>
<% end %>

<%= form_tag "/login" do %>
<%= label :username  , "Username"%>
<%= text_field_tag :username %>
<%= label :name  , "Username"%>
<%= text_field_tag :name %>
<%= label :password , "Password" %>
<%= password_field_tag :password%>
<%= submit_tag "Login" %>





<% end %> 

<br><%= button_to "Create User", new_user_path, method: :get%>