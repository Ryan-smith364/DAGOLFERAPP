
<%= form_for @review do |r|%>
<%= r.label:user %><br>
<%= r.collection_select :user, User.all , :id , :name %><br>

<%= r.label:course %><br>
<%= r.collection_select :course, Course.all , :id , :name %><br>

<%= r.label:review_text %><br>
<%= r.text_area:review_text %><br>

<%= r.label:rating %><br>
<%= r.number_field:rating %><br>

<%= r.submit %>
<% end %>
